<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hotapk.fastandrutils">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.GET_TASKS" />
    <uses-permission android:name="android.permission.KILL_BACKGROUND_PROCESSES" />
    <uses-permission android:name="android.permission.RESTART_PACKAGES" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/f_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".FSplashActivity"
            android:theme="@style/splashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".FMainActivity" />
        <activity android:name=".FAppInforActivity" />
        <activity android:name=".FReadAssetsActivity" />
        <activity android:name=".FCleanCacheActivity" />
        <activity android:name=".FCheckDoubleClickActivity" />
        <activity android:name=".FConvertActivity" />
        <activity android:name=".FDeviceActivity" />
        <activity
            android:name=".FKeyBoradActivity"
            android:windowSoftInputMode="adjustResize|stateAlwaysHidden" />
        <activity android:name=".FToastActivity" />
        <activity android:name=".FStatusBarActivity" />
        <activity android:name=".statusbar.TransBarActivity" />
        <activity android:name=".statusbar.SysBarActivity" />
        <activity android:name=".statusbar.PaddingTopBarActivity" />
        <activity android:name=".statusbar.MarginTopBarActivity" />
        <activity android:name=".statusbar.DrawerBarActivity" />
        <activity android:name=".statusbar.ChangeBarActivity" />
        <activity android:name=".FPermissionActivity" />
        <activity android:name=".FLogActivity" />
        <activity android:name=".FDateActivity" />
        <activity android:name=".FValidatorActivity" />
        <activity android:name=".FImageActivity" />
        <activity android:name=".FNetworkActivity" />
        <activity android:name=".FEmptyActivity" />
        <activity android:name=".FDialogFragmentActivity" />
        <activity android:name=".TabLayoutActivity" />
        <activity android:name=".LinearLayoutActivity" />
        <activity android:name=".FButtonActivity" />
        <activity android:name=".DialogActivity" />
        <activity android:name=".FShareActivity"></activity>
    </application>

</manifest>